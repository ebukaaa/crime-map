{
  "name": "crime-map",
  "version": "4.0.0",
  "author": {
    "name": "Ebuka Isiadinso",
    "email": "ebukaa@protonmail.com"
  },
  "private": true,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "git@github.com:ebukaaa/crime-map.git",
  "license": "MIT",
  "scripts": {
    "add-android": "npx cap add android",
    "add-electron": "npx cap add electron",
    "add-ios": "npx cap add ios",
    "audit": "npm audit fix",
    "build": "next build && next export && npm run copy-capacitor",
    "clear-react-import": "npx react-codemod update-react-imports",
    "copy-capacitor": "npx cap copy",
    "dev": "next dev -p 8532",
    "init-capacitor": "npx cap init",
    "lint": "eslint --ext .js,.jsx pages/",
    "open-android": "npx cap open android",
    "open-electron": "npx cap open electron",
    "open-ios": "npx cap open ios ",
    "outdated": "npx npm-check-updates -u",
    "set": "npm i",
    "start": "next start",
    "test": "jest",
    "telemetry-disable": "npx next telemetry disable",
    "telemetry-enable": "npx next telemetry enable",
    "telemetry-status": "npx next telemetry status",
    "update": "npm update",
    "upgrade": "sudo npm update npm -g"
  },
  "proxy": "http://localhost:9865",
  "dependencies": {
    "@capacitor/cli": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@capacitor/ios": "^2.4.7",
    "encoding": "^0.1.13",
    "leaflet": "^1.7.1",
    "next": "10.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "^3.1.0",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "sass": "^1.32.8",
    "supercluster": "^7.1.2",
    "swr": "^0.5.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@types/jest": "^26.0.21",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-next-dynamic": "^1.0.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
